{
    "name": "iono/container",
    "description": "spring framework style dependency injection container",
    "minimum-stability": "stable",
    "license": "MIT",
    "keywords": ["container", "field injection", "reflection", "annotation", "dependency injection"],
    "authors": [
        {
            "name": "yuuki.takezawa",
            "email": "yuuki.takezawa@comnect.jp.net"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-tokenizer": "*",
        "illuminate/filesystem": "5.*",
        "illuminate/container": "5.*",
        "nikic/php-parser": "1.0.*",
        "andrewsville/php-token-reflection": "1.*",
        "classpreloader/classpreloader": "1.*",
        "doctrine/annotations": "1.*",
        "doctrine/common": "2.*",
        "symfony/finder": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.*",
        "satooshi/php-coveralls": "dev-master",
        "facebook/xhprof": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Iono\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iono\\_TestContainer\\": [
                "tests/",
                "demo/"
            ]
        }
    },
    "bin": [
        "bin/init.container.php"
    ]
}
